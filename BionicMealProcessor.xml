<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="FS22_A_ProductionRevamp.RevampProductionPoint" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_Bionic Meat Processor</name>
        <functions>
            <function>$l10n_function_Bionic Meat Processor</function>
        </functions>
        <image>store_BionicMeatStation.png</image>
        <price>110000</price>
        <lifetime>1000</lifetime>
         <dailyUpkeep>50</dailyUpkeep>       
        <rotation>0</rotation>
        <brand>Lizard</brand>
        <species>placeable</species>
        <category>productionPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>factories</tab>
        </brush>
    </storeData>

    <base>
        <filename>BionicMeatStationTest.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
    </base>
	
 <!--   ClearAreas, LevelAreas, TestAreas, IndoorAreas, TipOcclusionAreas, UpdateAreas -->	
    <clearAreas>
        <clearArea startNode="clearAreas1Start" widthNode="clearAreas1Width" heightNode="clearAreas1Height"/>
    </clearAreas>
    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="75" maxEdgeAngle="30" >
        <levelAreas>
            <levelArea startNode="levelAreas1Start" widthNode="levelAreas1Width" heightNode="levelAreas1Height" />		
        </levelAreas>
    </leveling>
    <placement useRandomYRotation="false" useManualYRotation="true" placementPositionSnapSize="0.5" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
        </testAreas>
        <sounds>
            <place template="largeImp" />
        </sounds>
    </placement>
    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>
    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>
     <indoorAreas>
        <indoorArea startNode="indoorAreas1Start" widthNode="indoorAreas1Width" heightNode="indoorAreas1Height" />         
    </indoorAreas> 

 <!--   Hotspot, LoadedParts, TriggerMarkers -->	

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="2" teleportNode="teleportNode" />	
    </hotspots>

    <infoTrigger triggerNode="infoTrigger" />

    <triggerMarkers>
        <triggerMarker node="0>2|1|3" filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH" adjustToGround="true" />
        <triggerMarker node="0>2|1|1" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
        <triggerMarker node="0>2|1|2" filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD" adjustToGround="true" />
        <triggerMarker node="0>2|1|0" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

 <!--  Production   --> 	

    <productionPoint>
        <productions sharedThroughputCapacity="false">		
			<production  id="Stem cells" name="%s (%s)" params="$l10n_fillType_STEMCELLS|$l10n_fillType_Milk" cyclesPerHour="2" costsPerActiveHour="10" >
                <inputs>
					<input fillType="MILK" amount="100"/>
                </inputs>
                <outputs>
                    <output fillType="STEMCELLS" amount="20" />                   
                </outputs>
            </production>
			<production  id="UHTMILK" name="%s (%s)" params="$l10n_fillType_UHTMILK|$l10n_fillType_Milk" cyclesPerHour="2" costsPerActiveHour="10" >
                <inputs>
					<input fillType="MILK" amount="100"/>
                </inputs>
                <outputs>                  
                    <output fillType="UHTMILK" amount="95" /> 
                </outputs>
            </production>
			<production id="Bionic Meat" name="%s (%s)" params="$l10n_fillType_BIONICMEAT|$l10n_fillType_STEMCELLS+NUTRIENT" cyclesPerHour="2" costsPerActiveHour="10">
                <inputs>
					<input fillType="STEMCELLS" amount="20"/>
					<input fillType="NUTRIENTSOLUTION" amount="20"/>
                </inputs>
                <outputs>
                    <output fillType="BIONICMEAT" amount="100" />
                </outputs>
            </production>	            			 
        </productions>

        <storage isExtension="false" fillLevelSyncThreshold="50">
            <capacity fillType="MILK"      			capacity="25000" />
            <capacity fillType="STEMCELLS" 			capacity="5000" hideFromMenu="true"/>
			<capacity fillType="UHTMILK"    	    capacity="20000" />
			<capacity fillType="BIONICMEAT"    	    capacity="20000" />
			<capacity fillType="NUTRIENTSOLUTION"   capacity="20000" />
        </storage>

        <ProductionDisplays>
            <ProductionDisplay fillType="MILK" node="0>8|1|1" size="0.03" alignment="CENTER" mask="000000" color="1 1 1 1" emissiveScale="1.8" font="LICENSE_PLATE_DE"/>             
            <ProductionDisplay fillType="MILK" node="0>8|0|1" size="0.03" alignment="CENTER" mask="000000" color="1 1 1 1" emissiveScale="1.8" font="LICENSE_PLATE_DE"/>             
            <ProductionDisplay fillType="STEMCELLS" node="0>8|1|2" size="0.03" alignment="CENTER" mask="000000" color="1 1 1 1" emissiveScale="1.8" font="LICENSE_PLATE_DE"/>                                                                           
            <ProductionDisplay fillType="NUTRIENTSOLUTION" node="0>8|1|3" size="0.03" alignment="CENTER" mask="000000" color="1 1 1 1" emissiveScale="1.8" font="LICENSE_PLATE_DE"/>                                                                           
            <ProductionDisplay fillType="BIONICMEAT" node="0>8|1|4" size="0.03" alignment="CENTER" mask="000000" color="1 1 1 1" emissiveScale="1.8" font="LICENSE_PLATE_DE"/>                                                                           
            <ProductionDisplay fillType="UHTMILK" node="0>8|1|5" size="0.03" alignment="CENTER" mask="000000" color="1 1 1 1" emissiveScale="1.8" font="LICENSE_PLATE_DE"/>                                                                           
        </ProductionDisplays> 

        <sellingStation node="sellingStation" appearsOnStats="true" allowMissions="false" supportsExtension="true" hideFromPricesMenu="true">
            <unloadTrigger exactFillRootNode="unloadTriggerMilk"                       fillTypes="MILK"/>
            <unloadTrigger exactFillRootNode="unloadTriggerNutrientSolution"            fillTypes="NUTRIENTSOLUTION STEMCELLS"/>                                                                                      			
        </sellingStation>

         <palletSpawner node="palletSpawner">
            <spawnPlaces >
                <spawnPlace startNode="spawnSpace1Start" fillTypes="UHTMILK NUTRIENTSOLUTION BIONICMEAT STEMCELLS" />
                <spawnPlace startNode="spawnSpace2Start"  fillTypes="UHTMILK NUTRIENTSOLUTION BIONICMEAT STEMCELLS" />                
                <spawnPlace startNode="spawnSpace3Start" fillTypes="UHTMILK NUTRIENTSOLUTION BIONICMEAT STEMCELLS" />
                <spawnPlace startNode="spawnSpace4Start"  fillTypes="UHTMILK NUTRIENTSOLUTION BIONICMEAT STEMCELLS" />                
            </spawnPlaces>            
        </palletSpawner>

        <playerTrigger node="playerTrigger" />

        <sounds>
            <idle template="dairyIdle" linkNode="SoundIdle" outerRadius="10" volumeScale="0.5"/>
            <active template="dairyWork" linkNode="SoundActive" outerRadius="10" volumeScale="0.5"/>

        </sounds>
		
		<animationNodes>
            <animationNode node="0>4|0|0|4|3" rotSpeed="150"  	rotAxis="2" turnOnFadeTime="1" turnOffFadeTime="4"/>
            <animationNode node="0>4|0|0|4|2" rotSpeed="150"  	rotAxis="3" turnOnFadeTime="1" turnOffFadeTime="4"/>
            <animationNode node="0>4|0|0|4|1" rotSpeed="150"  	rotAxis="3" turnOnFadeTime="1" turnOffFadeTime="4"/>
        </animationNodes>        
    </productionPoint>

 <!--  Lights  -->

    <lights>
        <group weatherRequiredFlags="NIGHT" />
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode1" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode2" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode3" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode4" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode5" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode6" groupIndex="1"/>                                
           <sharedLight filename="lights/wallLight25w.xml" linkNode="lightNode7" groupIndex="1"/>                                                                                                                                                                                     
        <realLights>
            <low>
                <light node="realLightsLow" groupIndex="1"/> 
            </low>
            <high>
                <light node="realLightsHigh" groupIndex="1"/>
            </high>
        </realLights>
    </lights>  

	<animatedObjects>
         <animatedObject saveId="0>5" >
            <animation duration="5">
                <part node="0>5|0|0">
                    <keyFrame time="0.0" translation="0 0 0"/>
                    <keyFrame time="1.0" translation="0 2.566 0"/>
                </part>
                <part node="0>5|0|1">
                    <keyFrame time="0.0" translation="0 0 0"/>
                    <keyFrame time="1.0" translation="0 2.566 0"/>
                </part>
                <part node="0>5|0|2">
                    <keyFrame time="0.0" translation="0 0 0"/>
                    <keyFrame time="0.5" translation="0 0 0"/>
                    <keyFrame time="1.0" translation="0 1.25 0"/>
                </part>
                <shader node="0>5|0|0" parameterName="visibilityCutThreshold">
                    <keyFrame time="0.0" values="0 - - - " />
                    <keyFrame time="0.5" values="0.35 - - - " />
                    <keyFrame time="1.0" values="0.75 - - -" />
                </shader>
            </animation>
            <controls triggerNode="0>5|1" posAction="ACTIVATE_HANDTOOL" posText="OpenGate" negText="CloseGate" />
            <sounds>
                <moving template="slidingGateMetalLoop" linkNode="0>11|0"/>
                <posEnd template="metalDoorsOpen" linkNode="0>11|0"/>
                <negEnd template="slidingGateMetalClose" linkNode="0>11|0"/>
            </sounds> 
        </animatedObject> 
         <animatedObject saveId="0>5" >
            <animation duration="2">
                <part node="0>5|3">
                    <keyFrame time="0.0" translation="-3.087 1.178 3.421"/>
                    <keyFrame time="1.0" translation="-3.087 1.178 2.282"/>
                </part>
            </animation>
            <controls triggerNode="0>5|4" posAction="ACTIVATE_HANDTOOL" posText="OpenGate" negText="CloseGate" />
             <sounds>
                <moving file="sounds/electrischeTuer.ogg" linkNode="0>11|1"/>
            </sounds>  
        </animatedObject> 
         <animatedObject saveId="0>5" >
            <animation duration="2">
                <part node="0>6|14|2">
                    <keyFrame time="0.0" visibility="false"/>
                    <keyFrame time="0.5" visibility="true"/>
                </part>
                <part node="0>6|14|4">
                    <keyFrame time="0.0" visibility="false"/>
                    <keyFrame time="0.5" visibility="true"/>
                </part>
            </animation>
            <controls triggerNode="0>6|14|3" posAction="ACTIVATE_HANDTOOL" posText="LightOn" negText="LightOff" />
             <sounds>
            <sounds>
                <toggle template="lizardFloodLight" innerRadius="50" outerRadius="100" volumeScale="0.2"/>
            </sounds>
            </sounds>  
        </animatedObject> 
    </animatedObjects>

 <!--  I3d Mapping  -->

     <i3dMappings>
       <i3dMapping id="BionicMeatProduction" node="0>" />    
       <i3dMapping id="levelAreas1Start" node="0>0|0|0" />
       <i3dMapping id="levelAreas1Width" node="0>0|0|0|0" />
       <i3dMapping id="levelAreas1Height" node="0>0|0|0|1" />    
       <i3dMapping id="clearAreas1Start" node="0>0|1|0" />
       <i3dMapping id="clearAreas1Width" node="0>0|1|0|0" />
       <i3dMapping id="clearAreas1Height" node="0>0|1|0|1" />   
       <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>0|2|0" />
       <i3dMapping id="tipOcclusionUpdateArea1End" node="0>0|2|0|0" />    
       <i3dMapping id="testArea1Start" node="0>0|3|0" />
       <i3dMapping id="testArea1End" node="0>0|3|0|0" /> 
       <i3dMapping id="indoorAreas1Start" node="0>0|4|0" />
       <i3dMapping id="indoorAreas1Width" node="0>0|4|0|0" />
       <i3dMapping id="indoorAreas1Height" node="0>0|4|0|1" />         
       
       <i3dMapping id="productionPoint" node="0>1" />
       <i3dMapping id="infoTrigger" node="0>1|0" />
       <i3dMapping id="playerTrigger" node="0>1|1" />
       <i3dMapping id="playerTriggerMarker" node="0>1|2" />
       <i3dMapping id="teleportNode" node="0>1|3" />

       <i3dMapping id="sellingStation" node="0>2" />    
       <i3dMapping id="unloadTriggerMilk" node="0>2|0|0" />
       <i3dMapping id="unloadTriggerNutrientSolution" node="0>2|0|1" />
       <i3dMapping id="unloadingTriggerMarkerMilk" node="0>2|1|0" />
       <i3dMapping id="unloadingTriggerMarkerNutrientSolution" node="0>2|1|1" />
       <i3dMapping id="unloadingTriggerMarkerMeat" node="0>2|1|2" />
  
       <i3dMapping id="palletSpawner" node="0>3" />
       <i3dMapping id="spawnSpace1Start" node="0>3|0" />
       <i3dMapping id="spawnSpace1End" node="0>3|0|0" />
       <i3dMapping id="spawnSpace2Start" node="0>3|1" />
       <i3dMapping id="spawnSpace2End" node="0>3|1|0" />
       <i3dMapping id="spawnSpace3Start" node="0>3|2" />
       <i3dMapping id="spawnSpace3End" node="0>3|2|0" />
       <i3dMapping id="spawnSpace4Start" node="0>3|3" />
       <i3dMapping id="spawnSpace4End" node="0>3|3|0" />
    
       <i3dMapping id="lightNode"  node="0>6|0" /> 
       <i3dMapping id="lightNode1" node="0>6|1" /> 
       <i3dMapping id="lightNode2" node="0>6|2" /> 
       <i3dMapping id="lightNode3" node="0>6|3" /> 
       <i3dMapping id="lightNode4" node="0>6|4" /> 
       <i3dMapping id="lightNode5" node="0>6|5" /> 
       <i3dMapping id="lightNode6" node="0>6|6" /> 
       <i3dMapping id="lightNode7" node="0>6|7" /> 
    
       <i3dMapping id="realLights" node="0>6|14" />
       <i3dMapping id="realLightsLow" node="0>6|14|0" />
       <i3dMapping id="realLightsHigh" node="0>6|14|1" /> 

       <i3dMapping id="SoundActive" node="0>11|2" /> 
       <i3dMapping id="SoundIdle" node="0>11|3" /> 
     </i3dMappings>

</placeable>
