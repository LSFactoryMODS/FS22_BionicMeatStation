<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_fillType_NutrientSolution</name> 
         <functions>
            <function>$l10n_function_NutrientSolution</function>
        </functions>
        <specs>
            <weight ignore="true"/>
        </specs>               
        <image>Paletten/store_NutrientSolution.png</image>
		<price>1200</price>        
        <allowLeasing>false</allowLeasing>
        <canBeSold>false</canBeSold>
        <rotation>0</rotation>
        <brand>LIZARD</brand>
        <category>pallets</category>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>50688</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>6912</indexBufferMemoryUsage>
        <textureMemoryUsage>1048576</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>


    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>Paletten/nutrientSolutionPallet.i3d</filename>
        <size width="1.4" length="1" height="0.5" />
        <canBeReset>false</canBeReset>
        <components maxMass="850">
            <component centerOfMass="0 0.1 0" solverIterationCount="10" mass="200"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="NUTRIENTSOLUTION" startFillType="NUTRIENTSOLUTION" capacity="400" startFillLevel="400" ignoreFillLimit="true"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="500" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="pallet_vis">
        <content>
            <object node="Nutrient1" tensionBeltNode="Nutrient1Tension"/>
            <object node="Nutrient2" tensionBeltNode="Nutrient2Tension"/>
            <object node="Nutrient3" tensionBeltNode="Nutrient3Tension"/>
            <object node="Nutrient4" tensionBeltNode="Nutrient4Tension"/>
            <object node="Nutrient5" tensionBeltNode="Nutrient5Tension"/>
            <object node="Nutrient6" tensionBeltNode="Nutrient6Tension"/>
            <object node="Nutrient7" tensionBeltNode="Nutrient7Tension"/>
            <object node="Nutrient8" tensionBeltNode="Nutrient8Tension"/>	           	
        </content>

        <sounds>
            <unload template="palletsFillPlastic" pitchScale="0.8" />
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="5" additionalMountDistance="0.85" jointTransY="0.11" jointLimitToRotY="true"/>

<i3dMappings>
  <i3dMapping id="NutrientSolution_main_component1" node="0>" />
  <i3dMapping id="pallet_vis" node="0>0" />
  <i3dMapping id="collisions" node="0>0|0" />
  <i3dMapping id="floorCollision01" node="0>0|0|0" />
  <i3dMapping id="floorCollision02" node="0>0|0|1" />
  <i3dMapping id="content" node="0>0|1" />
  <i3dMapping id="Nutrient1" node="0>0|1|0" />
  <i3dMapping id="Nutrient1Tension" node="0>0|1|0|0" />
  <i3dMapping id="Nutrient2" node="0>0|1|1" />
  <i3dMapping id="Nutrient2Tension" node="0>0|1|1|0" />
  <i3dMapping id="Nutrient3" node="0>0|1|2" />
  <i3dMapping id="Nutrient3Tension" node="0>0|1|2|0" />
  <i3dMapping id="Nutrient4" node="0>0|1|3" />
  <i3dMapping id="Nutrient4Tension" node="0>0|1|3|0" />
  <i3dMapping id="Nutrient5" node="0>0|1|4" />
  <i3dMapping id="Nutrient5Tension" node="0>0|1|4|0" />
  <i3dMapping id="Nutrient6" node="0>0|1|5" />
  <i3dMapping id="Nutrient6Tension" node="0>0|1|5|0" />
  <i3dMapping id="Nutrient7" node="0>0|1|6" />
  <i3dMapping id="Nutrient7Tension" node="0>0|1|6|0" />
  <i3dMapping id="Nutrient8" node="0>0|1|7" />
  <i3dMapping id="Nutrient8Tension" node="0>0|1|7|0" />
  <i3dMapping id="dischargeNode" node="0>0|2" />
  <i3dMapping id="dynamicMountTrigger" node="0>0|3" />
</i3dMappings>
</vehicle>